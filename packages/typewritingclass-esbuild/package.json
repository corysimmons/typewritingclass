{
  "name": "typewritingclass-esbuild",
  "version": "0.2.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/corysimmons/typewritingclass",
    "directory": "packages/typewritingclass-esbuild"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "esbuild": ">=0.17.0"
  },
  "dependencies": {
    "typewritingclass-compiler": "workspace:*",
    "typewritingclass": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "esbuild": "^0.25.0",
    "typescript": "^5.7.3"
  }
}
